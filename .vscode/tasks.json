{
    "version": "2.0.0",
    "tasks": [
      {
        "type": "shell",
        "label": "build_algorithm",
        "command": "/usr/bin/gcc-7",
        "args": [
          "-Wall", "-g" ,"-O0",  "-std=gnu99", "-Wno-unused-function",
           "-I", "${workspaceFolder}/src",
           "${workspaceFolder}/src/tests/test_algorithm.c",
           "${workspaceFolder}/src/algorithm/array.c",
           "-o", "${workspaceFolder}/bin/test_algorithm"
        ],
        "options": {
          "cwd": "/usr/bin",

        },
        "problemMatcher": ["$gcc"],
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "detail": "Task generated by Debugger."
      },
      {
        "type": "shell",
        "label": "build_link",
        "command": "/usr/bin/gcc-7",
        "args": [
            "-Wall", "-g" ,"-O0", "-Werror", "-std=gnu99", "-Wno-unused-function",
            "-I", "${workspaceFolder}/src",
            "${workspaceFolder}/src/tests/test_parseElf.c",
            "${workspaceFolder}/src/common/print.c",
            "${workspaceFolder}/src/common/convert.c",
            "${workspaceFolder}/src/linker/parseElf.c",
            "${workspaceFolder}/src/linker/staticLinker.c",
            "-o", "${workspaceFolder}/bin/test_parselink"
        ],
        "options": {
          "cwd": "/usr/bin",
        },
        "problemMatcher": ["$gcc"],
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "detail": "Task generated by Debugger."
      }
    ]
}